@using Project.Constraints.Services
@using Project.Models.Entities
@page "/user/index"
@inject ICustomSettingProvider SettingProvider

@if (userPageType != null)
{
    <DynamicComponent Type="userPageType"></DynamicComponent>
}
else
{
    
}

@code {
    Type? userPageType;
    protected override void OnInitialized()
    {
        base.OnInitialized();
        userPageType = SettingProvider.GetUserPageType();
    }
}


