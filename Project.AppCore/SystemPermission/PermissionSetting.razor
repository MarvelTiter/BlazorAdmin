@page "/permission"
@inherits ModelPage<Power, GenericRequest<Power>>
@TableFragment

@code {
    string? selectedIcon;
    RenderFragment<object?> IconSelect()
    {
        return data =>
        {
            var p = data as Power;
            selectedIcon = p?.Icon;
            return@<Project.AppCore.SystemPermission.Component.PowerIconSelector @bind-Value="@selectedIcon"></Project.AppCore.SystemPermission.Component.PowerIconSelector>
    ;
        };
    }
}
